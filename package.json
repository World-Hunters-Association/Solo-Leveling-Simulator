	"dependencies": {
		"@devtomio/plugin-botlist": "^1.2.0",
		"@discordjs/builders": "^0.13.0",
		"@discordjs/collection": "^0.4.0",
		"@kaname-png/plugin-statcord": "^1.3.3",
		"@sapphire/decorators": "^4.0.2",
		"@sapphire/discord-utilities": "^2.6.0",
		"@sapphire/discord.js-utilities": "^4.4.0",
		"@sapphire/fetch": "^2.0.4",
		"@sapphire/framework": "next",
		"@sapphire/plugin-api": "^3.1.3",
		"@sapphire/plugin-hmr": "^1.2.0",
		"@sapphire/plugin-i18next": "^2.4.2",
		"@sapphire/plugin-logger": "^2.1.2",
		"@sapphire/plugin-subcommands": "^2.1.3",
		"@sapphire/time-utilities": "^1.5.2",
		"@sapphire/type": "^2.1.2",
		"@sapphire/utilities": "^3.2.1",
		"chokidar-cli": "^3.0.0",
		"colorette": "^2.0.16",
		"discord.js": "^13.6.0",
		"dotenv-cra": "^3.0.2",
		"mongodb": "^4.5.0",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.3.3",
		"@sapphire/prettier-config": "^1.2.9",
		"@sapphire/ts-config": "^3.1.8",
		"@types/node": "^17.0.10",
		"@types/ws": "^8.2.2",
		"commitlint": "^16.2.3",
		"cpy-cli": "^4.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.13.0",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "^12.3.8",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"tsc-watch": "^4.6.0",
		"typescript": "^4.5.5"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"start": "node dist/client.js",
		"dev": "run-p watch chokidar start",
		"watch:postbuild": "tsc-watch --onSuccess \"cpy src/languages/**/*.json dist/languages/\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"commit": "git-cz",
		"prepare": "husky install .github/.husky",
		"postbuild": "cpy src/languages/**/*.json dist/languages/",
		"chokidar": "chokidar \"src/languages/**/*.json\" -c \"cpy src/languages/**/*.json dist/languages/\""
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.0"
}
