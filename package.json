{
	"name": "solo-leveling-simulator",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "World Hunters Association",
	"license": "MIT",
	"dependencies": {
		"@devtomio/plugin-botlist": "^1.2.0",
		"@discordjs/builders": "^0.13.0",
		"@discordjs/collection": "^0.6.0",
		"@kaname-png/plugin-statcord": "^1.3.3",
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/discord-utilities": "^2.10.3",
		"@sapphire/discord.js-utilities": "^4.9.7",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-hmr": "^1.2.0",
		"@sapphire/plugin-i18next": "^2.5.0",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/utilities": "^3.6.2",
		"chokidar-cli": "^3.0.0",
		"colorette": "^2.0.16",
		"discord.js": "^13.6.0",
		"dotenv-cra": "^3.0.2",
		"mongodb": "^4.5.0",
		"npm-run-all": "^4.1.5",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^16.2.4",
		"@sapphire/eslint-config": "^4.3.4",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^17.0.10",
		"@types/ws": "^8.5.3",
		"commitlint": "^16.2.4",
		"cpy-cli": "^4.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.14.0",
		"husky": "^7.0.4",
		"jest": "^28.0.2",
		"lint-staged": "^12.4.1",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.6.3"
	},
	"scripts": {
		"commit": "git-cz",
		"prepare": "husky install .github/.husky",
		"watch": "tsc -w",
		"build:p": "npm run build && cp -R src/languages dist/languages",
		"chokidar:p": "chokidar \"src/languages/**/*.json\" -c \"cp -R src/languages dist/languages\"",
		"start:p": "node dist/index.js",
		"staging": "npm run build:p && run-p watch chokidar:p start:dev",
		"build": "tsc",
		"start": "run-p watch chokidar:p start:p",
		"build:dev": "npm run build&cpy src/languages/**/*.json dist/languages/",
		"chokidar:dev": "chokidar \"src/languages/**/*.json\" -c \"cpy src/languages/**/*.json dist/languages/\"",
		"prebuild:dev": "rd /s /q dist",
		"start:dev": "node dist/client.js",
		"dev": "npm run build:dev&run-p watch chokidar:dev start:dev"
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.0"
}
